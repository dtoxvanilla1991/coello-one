{
  "name": "coello-one",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bun --bun next dev --turbopack",
    "build": "bun --bun next build",
    "start": "bun --bun next start",
    "lint": "eslint . --max-warnings=0",
    "test": "bun test --preload ./test-setup.ts",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "verify:locales": "bun scripts/verifyTranslations.ts",
    "generate:antd-css": "bun ./scripts/genAntdCss.ts",
    "predev": "cross-env NODE_ENV=development bun run generate:antd-css",
    "prebuild": "cross-env NODE_ENV=production bun run generate:antd-css"
  },
  "engines": {
    "node": ">=24.12.0",
    "bun": ">=1.2.21"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^2.0.1",
    "@ant-design/icons": "6",
    "@ant-design/nextjs-registry": "^1.3.0",
    "@ant-design/static-style-extract": "^2.0.2",
    "@hookform/resolvers": "^5.2.2",
    "antd": "6",
    "drizzle-orm": "^0.45.1",
    "jotai": "^2.16.1",
    "mysql2": "^3.16.0",
    "next": "16.1.1",
    "react": "^19.2.3",
    "react-countdown": "^2.3.6",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.69.0",
    "server-only": "^0.0.1",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@happy-dom/global-registrator": "^20.0.11",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "baseline-browser-mapping": "^2.9.11",
    "bun-types": "^1.3.5",
    "cross-env": "^10.1.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.1",
    "eslint-plugin-tailwindcss": "^4.0.0-beta.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
