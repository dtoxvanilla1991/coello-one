{
  "name": "coello-one",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bun --bun next dev --turbopack",
    "build": "bun --bun next build",
    "start": "bun --bun next start",
    "lint": "eslint . --max-warnings=0",
    "test": "bun test --preload ./test-setup.ts",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "verify:locales": "bun scripts/verifyTranslations.ts",
    "generate:antd-css": "bun ./scripts/genAntdCss.ts",
    "predev": "cross-env NODE_ENV=development bun run generate:antd-css",
    "prebuild": "cross-env NODE_ENV=production bun run generate:antd-css"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.24.0",
    "@ant-design/nextjs-registry": "^1.1.0",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "antd": "^6.1.1",
    "jotai": "^2.15.0",
    "next": "16.1.0",
    "react": "^19.2.0",
    "react-countdown": "^2.3.6",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.65.0",
    "server-only": "^0.0.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@happy-dom/global-registrator": "^20.0.0",
    "@tailwindcss/postcss": "^4.1.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "cross-env": "^10.1.0",
    "eslint": "^9.38.0",
    "eslint-config-next": "^16.0.0",
    "eslint-plugin-tailwindcss": "^4.0.0-beta.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3"
  }
